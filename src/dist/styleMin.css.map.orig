","file":"dist/styleMin.css","sourcesContent":["\n.rootForm{\n    //border: 1px solid #fff;\n    padding: 10px;\n    margin-bottom: 10px;\n    background: rgb(36, 39, 56);\n    box-shadow: 5px 5px 3px rgba(0,0,0,0.4);\n    position: relative;\n}\n\n.documentForm{\n    @include flex-std();\n    width: 100%;\n}\n\n.dropForm{\n    cursor: pointer;\n    position: relative;\n    //border: 1px solid #fff;\n    padding: 10px;\n    width: 130px;\n    margin-right: 10px;   \n    background: rgb(69, 78, 97);\n    box-shadow: 5px 5px 5px rgba(0,0,0,0.5);\n    &:hover{\n        background: rgb(87, 90, 94);\n    }\n    p{\n        margin: 2px;\n    }\n    .rootBox{\n        position: absolute;\n        padding: 3px;\n        background: rgb(243, 232, 167);\n        color: #000;\n        opacity: 0;\n        transition: opacity .2s .5s ease;\n        border-radius: 5px;\n        pointer-events: none;\n        z-index: map-get($dimention,rootBox);\n    }\n    &:hover > .rootBox{\n        opacity: 1;\n    }\n}\n\n.dropForm__rootName{\n    //display: block;\n    word-break: break-all;\n}\n\n.dropOver{\n    background: rgb(164, 174, 194);\n}","@mixin flex-std($content:flex-start){\n    display: flex;\n    justify-content: $content;\n}\n\n@mixin formBall($size:20px){\n    width: $size;\n    height: $size;\n    border-radius: 50%;\n}\n\n@mixin selectInit(){\n    appearance: none;\n    border: none;\n    &:focus{\n        outline: none;\n    }\n}\n\n@mixin onCenter($top:50%,$left:50%,$transX:-50%,$transY:-50%,$add:null){\n    position: absolute;\n    top: $top;\n    left: $left;\n    transform: translate($transX , $transY) $add;\n}","$dimention:(\n    rootBox:5,\n    loadColver:20,\n)",".ButtonRange{\n    ul{\n        @include flex-std();\n        li{\n            margin-right: 10px;\n        }\n    }\n}",".optionsMain{\n    ul{\n        li{\n            margin-bottom: 10px;\n        }\n    }\n}",".watching{\n    position: absolute;\n    width: 100%;\n    height: 100%;\n    top: 0;\n    left: 0;\n    background: rgba(0,0,0,0.7);\n    z-index: 20;\n    .watching__title{\n        display: block;\n        @include onCenter($top:85%);\n        opacity: 1;\n        animation: fading 1.3s linear infinite;\n    }\n    .watching__circle{\n        width: 120px;\n        height: 120px;\n        border: 2px solid rgba(255,255,255,0.4);\n        border-radius: 50%;\n        @include onCenter();\n        animation: seeing 3.1s linear infinite;\n        .watching__eye{\n            width: 40px;\n            height: 40px;\n            background: #fff;\n            border-radius: 50%;\n            @include onCenter($transX:-150%);\n            animation: eyeInside 3.1s linear infinite;\n            .watching__blackEye{\n                width: 20px;\n                height: 20px;\n                border-radius: 50%;\n                background: #000;\n                @include onCenter($transX:-120%);\n            }\n        }\n    }\n}\n\n@function timeLineTrans($X:-50%,$Y:-50%,$rotate:0deg){\n    @return translate($X , $Y) rotate($rotate);\n}\n\n@keyframes seeing{\n    0%{\n        transform: timeLineTrans();\n    }\n    100%{\n        transform: timeLineTrans($rotate:360deg);\n    }\n}\n\n@keyframes eyeInside{\n    0%{\n        transform: timeLineTrans($X:-150%);\n    }\n    100%{\n        transform: timeLineTrans($X:-150%,$rotate:360deg);\n    }\n}\n\n@keyframes fading{\n    0%{\n        opacity: 1;\n    }\n\n    50%{\n        opacity: 0.6;\n    }\n\n    100%{\n        opacity: 1;\n    }\n}","$checkDimention:(\n    base:1,\n    elm:2,\n    covered:3,\n    ball:5\n);\n\n.checkbox-doc{\n    z-index: map-get($checkDimention,base);\n    $checkColor:rgb(93, 95, 252);\n    margin: 10px 0px;\n    @include flex-std();\n    cursor: pointer;\n    width: 100px;\n    border: 1px solid #fff;\n    padding: 3px;\n    .checkbox-doc__input{\n        display: none;\n    }\n    .checkbox-doc__display{\n        z-index: map-get($checkDimention,ball);\n        width: 20px;\n        height: 20px;\n        border: 2px solid lighten($checkColor,30%);\n        border-radius: 2px;\n        position: relative;\n        margin-right: 5px;\n        .checkbox-doc__square{\n            width: 15px;\n            height: 15px;\n            background: $checkColor;\n            top: 50%;\n            left: 50%;\n            position: absolute;\n            transform: translate(-50%,-50%) scale(0);\n            transition: transform .3s ease;\n        }\n    }\n    .checkbox-doc__name{\n        z-index: map-get($checkDimention,elm);\n        display: block;\n    }\n}\n\n.checkbox-doc__input:checked + .checkbox-doc__display > .checkbox-doc__square{\n    transform: translate(-50%,-50%) scale(1);\n}\n\n\n\n.checkboxRange{\n    @include flex-std();\n}\n.checkbox-std{\n    z-index: map-get($checkDimention,base);\n    $hsize:25px;\n    $wsize:80px;\n    width: $wsize;\n    height: $hsize;\n    background: rgb(57, 59, 190);\n    display: block;\n    position: relative;\n    border-radius: 10px;\n    overflow: hidden;\n    cursor: pointer;\n    .checkbox-std__display{\n        z-index: map-get($checkDimention,ball);\n        position: absolute;\n        background: rgb(255, 255, 255);\n        @include formBall($hsize);\n        left: $wsize - $hsize;\n        transition: .3s ease;\n    }\n    .checkbox-std__cover{\n        z-index: map-get($checkDimention,covered);\n        position: absolute;\n        background: rgba(0,0,0,0.5);\n        width: 100%;\n        height: 100%;\n        top: 0;\n        left: 0;\n        opacity: 1;\n        transition: .3s ease;\n    }\n\n    .checkbox-std__text{\n        z-index: map-get($checkDimention,elm);\n        color: #999;\n        position: absolute;\n        @include onCenter();\n        transition: .3s ease;\n    }\n\n    .checkbox-std__input{\n        display: none;\n        &:checked + .checkbox-std__display{\n            left: 0px;\n        }\n        &:checked + .checkbox-std__display + .checkbox-std__cover{\n            opacity: 0;\n        }\n        &:checked + .checkbox-std__display + .checkbox-std__cover + .checkbox-std__text{\n            color: #fff;\n        }\n    }\n    \n}",".radiobox-doc{\n    @include flex-std();\n    padding: 5px;\n    cursor: pointer;\n    .radiobox-doc__input{\n        display: none;\n    }\n    .radiobox-doc__display{\n        @include formBall($size:20px);\n        background: rgb(170, 170, 170);\n        position: relative;\n        .radiobox-doc__ball{\n            @include formBall($size:15px);\n            background: rgb(150, 255, 102);\n            position: absolute;\n            top: 50%;\n            left: 50%;\n            transform: translate(-50% ,-50%) scale(0);\n            transition: transform .3s ease;\n        }\n    }\n}\n\n.radiobox-doc__input:checked + .radiobox-doc__display > .radiobox-doc__ball{\n    transform: translate(-50% ,-50%) scale(1);\n}",".button-watch{\n    $mainColor:rgb(69, 137, 240);\n    border: none;\n    width: 100px;\n    height: 20px;\n    background: $mainColor;\n    color: #ffffff;\n    cursor: pointer;\n    margin-bottom: 10px;\n    box-shadow: 2px 5px 0px darken($mainColor,30%);\n    border-radius: 3px;\n    &:active{\n        background: darken($mainColor,20%);\n    }\n    &:disabled{\n\n    }\n}\n\n.button-std{\n    $btnColor:rgb(112, 112, 112);\n    border: none;\n    width: 100px;\n    height: 20px;\n    background: $btnColor;\n    color: #ffffff;\n    cursor: pointer;\n    box-shadow: 2px 2px darken($btnColor,30%);\n    &:active{\n        background: darken($btnColor,20%);\n    }\n    &:disabled{\n        background: #000;\n        color: #555555;\n    }\n}",".selectBox-std{\n    position: relative;\n    $selectorWidht:200px;\n    width: $selectorWidht;\n    height: 30px;\n    overflow: hidden;\n    color: #000;\n    border-radius: 5px;\n    cursor: pointer;\n    .selectBox-std__input{\n        @include selectInit();\n        width: 100%;\n        height: 100%;\n        background: rgb(173, 173, 173);\n        cursor: pointer;\n        option{\n            position: absolute;\n            font-size: 15px;\n        }\n    }\n    .selectBox-std__plate{\n        position: absolute;\n        $plateWidth:30px;\n        width: $plateWidth;\n        height: 100%;\n        background: rgb(94, 94, 94);\n        top: 0;\n        left: $selectorWidht - $plateWidth;\n        pointer-events: none;\n        .selectBox-std__arrow{\n            $size:10px;\n            $line:2px solid rgb(26, 33, 34);\n            width: $size;\n            height: $size;\n            border-bottom: $line;\n            border-left: $line;\n            @include onCenter($top:40%,$add:rotate(-45deg));\n        }\n    }\n}","@import \"./mixin\";\n@import \"./LayerDimention\";\n@import \"../js/components/rootForm\";\n@import \"../js/components/buttonRange\";\n@import \"../js/pages/optionsForm\";\n@import \"../js/components/watchingCover\";\n\n@import \"../js/parts/check\";\n@import \"../js/parts/radio\";\n@import \"../js/parts/button\";\n@import \"../js/parts/select\";\n\nbody{\n    background: rgb(53, 52, 52);\n    color: #ffffff;\n    margin: 0;\n    font-family: \"Helvetica Neue\",\n    Arial,\n    \"Hiragino Kaku Gothic ProN\",\n    \"Hiragino Sans\",\n    Meiryo,\n    sans-serif;\n}\n\nul{\n    list-style: none;\n    padding: 0;\n}\n\n.head-top{\n    margin-top: 5px;\n    margin-bottom: 3px;\n    font-size: 30px;\n    font-weight: 200;\n}\n\n.head-small{\n    margin: 3px;\n    font-size: 15px;\n}\n\n.watchForm{\n    display: grid;\n    grid-template-rows: 50px 1fr 100px 50px;\n    grid-template-columns: 1fr;\n    grid-template-areas: \n        \"header\"\n        \"docList\"\n        \"buttonRange\"\n        \"footer\"\n    ;\n}\n\n.header{\n    grid-area: header;\n}\n\n.documentList{\n    grid-area:docList;\n}\n\n.ButtonRange{\n    grid-area: buttonRange;\n}\n\n.footer{\n    grid-area: footer;;\n}\n\n.none{\n    display: none;\n}",null],"sourceRoot":""}
